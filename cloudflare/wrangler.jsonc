{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "resume-template-engine",
  "main": "src/index.ts",
  "compatibility_date": "2025-04-01",
  "containers": [
    {
      "class_name": "ResumeEngine",
      "image": "../Dockerfile.cloudflare",
      "image_build_context": "../",
      "max_instances": 5,
      "instance_type": "standard-2"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "RESUME_ENGINE",
        "class_name": "ResumeEngine"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["ResumeEngine"]
    }
  ],
  "build": {
    "command": "npm run build"
  }
}
